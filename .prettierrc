{
  "semi": false,
  "trailingComma": "es5",
  "plugins": [
    "prettier-plugin-packagejson",
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss"
  ],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
